<p id="notice"><%= notice %></p>

<h1>Listing E Times</h1>

<table>
  <thead>
    <tr>
      <th>Start time</th>
      <th>End time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @e_times.each do |e_time| %>
      <tr>
        <td><%= e_time.start_time %></td>
        <td><%= e_time.end_time %></td>
        <td><%= link_to 'Show', e_time %></td>
        <td><%= link_to 'Edit', edit_e_time_path(e_time) %></td>
        <td><%= link_to 'Destroy', e_time, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New E time', new_e_time_path %>
